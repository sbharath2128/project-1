{
  "version": 2,
  "buildCommand": "uv sync",
  "outputDirectory": ".vercel/output",
  "installCommand": "uv sync",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "caraccessories_shop/asgi.py"
    }
  ]
}
