{
  "builds": [
    {
      "src": "caraccessories_shop/asgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "installCommand": "uv sync"
      }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "caraccessories_shop/asgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "caraccessories_shop/asgi.py"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/caraccessories_shop/asgi.py"
    }
  ]
}
